{
    "name": "gemini-desktop",
    "productName": "Gemini Desktop",
    "description": "A desktop wrapper for Google Gemini with enhanced features",
    "author": "Ben Wendell <github@benwendell.com>",
    "private": true,
    "version": "0.5.0",
    "type": "module",
    "main": "dist-electron/main/main.cjs",
    "scripts": {
        "dev": "vite",
        "prepare": "husky",
        "clean": "node scripts/clean.cjs",
        "build": "tsc && vite build",
        "build:electron": "tsc -p tsconfig.electron.json && node scripts/rename-electron-output.cjs",
        "lint": "eslint .",
        "format": "prettier --write .",
        "preview": "vite preview",
        "electron:dev": "npm run build:electron && concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:1420 && electron .\"",
        "electron:build": "npm run build && npm run build:electron && electron-builder --config config/electron-builder.config.cjs",
        "dist:mac-x64": "electron-builder --mac --x64 --publish never --config config/electron-builder.config.cjs",
        "dist:mac-arm64": "electron-builder --mac --arm64 --publish never --config config/electron-builder.config.cjs",
        "dist:win": "electron-builder --win --publish never --config config/electron-builder.config.cjs",
        "dist:linux": "electron-builder --linux --publish never --config config/electron-builder.config.cjs",
        "test": "vitest run --config config/vitest/vitest.config.ts",
        "test:watch": "vitest --config config/vitest/vitest.config.ts",
        "test:coverage": "vitest run --config config/vitest/vitest.config.ts --coverage",
        "test:electron": "vitest run --config config/vitest/vitest.electron.config.ts",
        "test:electron:coverage": "vitest run --config config/vitest/vitest.electron.config.ts --coverage",
        "test:coordinated": "vitest run --config config/vitest/vitest.coordinated.config.ts",
        "test:integration": "wdio run config/wdio/wdio.integration.conf.js",
        "test:integration:watch": "wdio run config/wdio/wdio.integration.conf.js --watch",
        "test:e2e": "node scripts/run-e2e-sequential.cjs",
        "test:e2e:spec": "wdio run config/wdio/wdio.conf.js",
        "test:e2e:group:startup": "wdio run config/wdio/wdio.group.startup.conf.js",
        "test:e2e:group:window": "wdio run config/wdio/wdio.group.window.conf.js",
        "test:e2e:group:menu": "wdio run config/wdio/wdio.group.menu.conf.js",
        "test:e2e:group:hotkeys": "wdio run config/wdio/wdio.group.hotkeys.conf.js",
        "test:e2e:group:quickchat": "wdio run config/wdio/wdio.group.quickchat.conf.js",
        "test:e2e:group:options": "wdio run config/wdio/wdio.group.options.conf.js",
        "test:e2e:group:theme": "wdio run config/wdio/wdio.group.theme.conf.js",
        "test:e2e:group:auth": "wdio run config/wdio/wdio.group.auth.conf.js",
        "test:e2e:group:tray": "wdio run config/wdio/wdio.group.tray.conf.js",
        "test:e2e:group:update": "wdio run config/wdio/wdio.group.update.conf.js",
        "test:e2e:group:stability": "wdio run config/wdio/wdio.group.stability.conf.js",
        "test:e2e:group:macos": "wdio run config/wdio/wdio.group.macos.conf.js",
        "test:e2e:lifecycle": "wdio run config/wdio/wdio.lifecycle.conf.js",
        "test:e2e:release": "wdio run config/wdio/wdio.release.conf.js",
        "test:e2e:all": "npm run test:e2e && npm run test:e2e:lifecycle",
        "test:all": "npm run test && npm run test:electron && npm run test:coordinated && npm run test:integration && npm run test:e2e:all",
        "docs": "typedoc --out docs src"
    },
    "dependencies": {
        "electron-log": "^5.4.3",
        "electron-updater": "^6.6.2",
        "framer-motion": "^12.23.26",
        "marked": "^17.0.1",
        "node-llama-cpp": "^3.14.5",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "turndown": "^7.2.2",
        "turndown-plugin-gfm": "^1.0.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@playwright/test": "^1.57.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@types/marked": "^5.0.2",
        "@types/react": "^19.2.10",
        "@types/react-dom": "^19.1.6",
        "@types/turndown": "^5.0.6",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.18",
        "@wdio/cli": "^9.22.0",
        "@wdio/globals": "^9.17.0",
        "@wdio/local-runner": "^9.22.0",
        "@wdio/mocha-framework": "^9.22.0",
        "@wdio/spec-reporter": "^9.20.0",
        "@wdio/types": "^9.20.0",
        "concurrently": "^9.2.1",
        "electron": "^39.3.0",
        "electron-builder": "^26.4.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "expect-webdriverio": "^5.6.1",
        "fast-check": "^4.5.3",

        "globals": "^17.2.0",
        "husky": "^9.1.7",
        "jsdom": "^27.4.0",
        "lint-staged": "^16.2.7",
        "prettier": "^3.8.1",


        "typedoc": "^0.28.16",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "^7.0.4",
        "vitest": "^4.0.15",
        "wait-on": "^9.0.3",
        "wdio-electron-service": "^9.2.1"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "overrides": {
        "tar": "^7.5.7"
    }
}
