{
  "name": "gemini-desktop",
  "productName": "Gemini Desktop",
  "description": "A desktop wrapper for Google Gemini with enhanced features",
  "author": "Ben Wendell <github@benwendell.com>",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "clean": "node scripts/clean.cjs",
    "build": "tsc && vite build",
    "build:electron": "tsc -p tsconfig.electron.json && node scripts/rename-electron-output.cjs",
    "preview": "vite preview",
    "electron:dev": "npm run build:electron && concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:1420 && electron .\"",
    "electron:build": "npm run build && npm run build:electron && electron-builder",
    "dist:mac-x64": "electron-builder --mac --x64 --publish never --config electron-builder.config.cjs",
    "dist:mac-arm64": "electron-builder --mac --arm64 --publish never --config electron-builder.config.cjs",
    "dist:win": "electron-builder --win --publish never --config electron-builder.config.cjs",
    "dist:linux": "electron-builder --linux --publish never --config electron-builder.config.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:electron": "vitest run --config vitest.electron.config.ts",
    "test:electron:coverage": "vitest run --config vitest.electron.config.ts --coverage",
    "test:coordinated": "vitest run --config vitest.coordinated.config.ts",
    "test:integration": "wdio run wdio.integration.conf.js",
    "test:integration:watch": "wdio run wdio.integration.conf.js --watch",
    "test:e2e": "node scripts/run-e2e-sequential.cjs",
    "test:e2e:lifecycle": "wdio run wdio.lifecycle.conf.js",
    "test:e2e:all": "npm run test:e2e && npm run test:e2e:lifecycle",
    "test:all": "npm run test && npm run test:electron && npm run test:coordinated && npm run test:integration && npm run test:e2e:all",
    "docs": "typedoc --out docs src"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.23.26",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@wdio/cli": "^9.21.1",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.21.0",
    "@wdio/mocha-framework": "^9.21.0",
    "@wdio/spec-reporter": "^9.20.0",
    "@wdio/types": "^9.20.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "expect-webdriverio": "^5.5.0",
    "jsdom": "^27.3.0",
    "typedoc": "^0.28.15",
    "typescript": "~5.9.3",
    "vite": "^7.0.4",
    "vitest": "^4.0.15",
    "wait-on": "^9.0.3",
    "wdio-electron-service": "^9.2.1"
  }
}